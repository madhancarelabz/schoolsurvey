        "jsCode": "// === CHATWOOT CONFIGURATION ===\nconst API_TOKEN = 'MJxWqHUBFJNvRaR1v6bBA8wE';\nconst ACCOUNT_ID = '153238';\nconst INBOX_ID = '97309';\nconst BASE_URL = `https://app.chatwoot.com/api/v1/accounts/${ACCOUNT_ID}`;\nconst headers = { 'Content-Type': 'application/json', 'api_access_token': API_TOKEN };\n\n// === GET DATA FROM PREVIOUS NODES ===\nconst session = $node[\"Postgres: Fetch Session\"].json;\nconst scores = $node[\"Postgres: Compute Final Scores\"].json;\nconst employeeId = session.employee_id;\nconst role = session.role;\nconst sessionId = session.session_id;\n\ntry {\n  // 1. Create Contact\n  const contactRes = await fetch(`${BASE_URL}/contacts`, {\n    method: 'POST', headers,\n    body: JSON.stringify({ name: `Employee ${employeeId}`, identifier: employeeId })\n  });\n  const contact = await contactRes.json();\n  const contactId = contact.payload?.contact?.id || contact.payload?.id;\n\n  // 2. Create Conversation\n  const convoRes = await fetch(`${BASE_URL}/conversations`, {\n    method: 'POST', headers,\n    body: JSON.stringify({ contact_id: contactId, inbox_id: parseInt(INBOX_ID) })\n  });\n  const convo = await convoRes.json();\n  const convoId = convo.id;\n\n  // 3. Message 1: Employee Metadata (L125)\n  await fetch(`${BASE_URL}/conversations/${convoId}/messages`, {\n    method: 'POST', headers,\n    body: JSON.stringify({\n      content: `ðŸ“‹ Survey Report\\nðŸ‘¤ Employee: ${employeeId}\\nðŸ·ï¸ Role: ${role}\\nðŸ”‘ Session: ${sessionId}\\nðŸ“… Date: ${new Date().toLocaleDateString('en-IN')}`,\n      message_type: 'outgoing'\n    })\n  });\n\n  // 4. Message 2: Scores (L127)\n  await fetch(`${BASE_URL}/conversations/${convoId}/messages`, {\n    method: 'POST', headers,\n    body: JSON.stringify({\n      content: `ðŸ“Š Scores\\nðŸ§  Mindset: ${scores.mindset_score}\\nðŸ”§ Toolset: ${scores.toolset_score}\\nðŸŽ¯ Skillset: ${scores.skillset_score}\\nðŸ“ˆ Total: ${scores.total_score}`,\n      message_type: 'outgoing'\n    })\n  });\n\n  // 5. Message 3: Summary + Recommendation (L129)\n  await fetch(`${BASE_URL}/conversations/${convoId}/messages`, {\n    method: 'POST', headers,\n    body: JSON.stringify({\n      content: `ðŸ’¡ Summary & Recommendation\\nðŸ“ ${scores.summary}\\nðŸŽ“ ${scores.recommendation}`,\n      message_type: 'outgoing'\n    })\n  });\n\n  // 6. Apply Labels/Tags (L131)\n  const scoreTag = scores.total_score >= 24 ? 'high-performer' : scores.total_score >= 12 ? 'moderate' : 'needs-training';\n  await fetch(`${BASE_URL}/conversations/${convoId}/labels`, {\n    method: 'POST', headers,\n    body: JSON.stringify({ labels: [role.toLowerCase(), scoreTag, 'ai-survey'] })\n  });\n\n  return [{ json: { chatwoot_status: 'pushed', conversation_id: convoId } }];\n\n} catch (error) {\n  return [{ json: { chatwoot_status: 'failed', error: error.message } }];\n}"
